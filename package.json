{
  "name": "fitgenie",
  "version": "0.0.1",
  "description": "FitGenie â€” Gemini-powered virtual try-on assistant for online shopping.",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build fitgenie",
    "build:background": "tsc --project tsconfig.background.json --noEmit && esbuild src/background/service-worker.ts --bundle --format=esm --platform=browser --outdir=dist/background --tsconfig=tsconfig.background.json --sourcemap",
    "build:content": "tsc --project tsconfig.content.json --noEmit && esbuild src/content-scripts/product-selector.ts --bundle --format=esm --platform=browser --outdir=dist/content-scripts --tsconfig=tsconfig.content.json --sourcemap",
    "build:extension": "npm run build:background && npm run build:content && npm run build",
    "lint": "ng lint",
    "start": "ng serve fitgenie"
  },
  "dependencies": {
    "@angular/animations": "^17.3.0",
